{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "(gdb) SRLauncher Launch",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/SRLauncher.exe",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [
        {
          "name": "LD_LIBRARY_PATH",
          "value": "C:/SFML-2.6.1/lib"
        }
      ],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "miDebuggerPath": "C:/Compillers/c++/MinGW64/bin/gdb.exe",
      "internalConsoleOptions": "neverOpen",
      "logging": {
        "engineLogging": true,
        "trace": true,
        "traceResponse": true
      }
    }
  ]
}
